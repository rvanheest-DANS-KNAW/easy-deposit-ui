{
  "id-type:DAI": {
    "title": "DAI",
    "viewName": "DAI",
    "format": "^([0-9]{4}-){3}[0-9]{3}[0-9xX]?$"
  },
  "id-type:ORCID": {
    "title": "ORCID",
    "viewName": "ORCID",
    "format": "(^([0-9]){15,16}X?$)|(^([0-9]{4}[ ]?){3}[0-9]{3}[0-9xX]$)",
    "baseURL": "https://pub.orcid.org/v2.1/search?q=\"%s\""
  },
  "id-type:ISNI": {
    "title": "ISNI",
    "viewName": "ISNI",
    "format": "^([0-9]{8,9})([0-9xX])$",
    "baseURL": "http://isni.org/isni/%s"
  }
}
